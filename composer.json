{
	"name": "aimeos/ai-laravel",
	"description": "Laravel adapter for Aimeos e-commerce integration",
	"keywords": ["aimeos", "adapter", "laravel", "shop", "e-commerce"],
	"homepage": "https://aimeos.org/Laravel",
	"type": "aimeos-extension",
	"license": "LGPL-3.0-or-later",
	"support": {
		"source": "https://github.com/aimeos/ai-laravel",
		"issues": "https://github.com/aimeos/ai-laravel/issues",
		"forum": "https://aimeos.org/help",
		"wiki": "https://aimeos.org/docs"
	},
	"prefer-stable": true,
	"minimum-stability": "dev",
	"require": {
		"php": "~7.1||~8.0",
		"nyholm/psr7": "~1.2",
		"laravel/framework": "^6.20.12||^7.30.4||^8.40.0",
		"aimeos/aimeos-core": "dev-master"
	},
	"require-dev": {
		"phpunit/phpunit": "~7.0||~8.0||~9.0"
	},
	"autoload": {
		"psr-4": {
			"Aimeos\\": "src"
		},
		"classmap": [
			"src"
		]
	},
	"autoload-dev": {
		"psr-4": {
			"Aimeos\\": "src"
		},
		"classmap": [
			"src"
		]
	}
}
