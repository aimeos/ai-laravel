{
	"name": "aimeos/ai-laravel",
	"description": "Aimeos extension for utilizing Laravel 4 and 5 components",
	"keywords": ["aimeos", "adapter", "laravel"],
	"homepage": "http://aimeos.org",
	"type": "aimeos-extension",
	"license": "LGPL-3.0",
	"support": {
		"source": "https://github.com/aimeos/ai-laravel",
		"issues": "https://github.com/aimeos/ai-laravel/issues",
		"forum": "https://aimeos.org/help",
		"wiki": "https://aimeos.org/docs"
	},
	"minimum-stability": "dev",
	"prefer-stable": true,
	"require": {
		"php": ">=5.3.0"
	},
	"require-dev": {
		"laravel/framework": ">=4.0"
	},
	"autoload": {
		"psr-0": {
			"MW": "lib/custom/src"
		},
		"classmap": [
			"lib/custom/src"
		]
	}
}